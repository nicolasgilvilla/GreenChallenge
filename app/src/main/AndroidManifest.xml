<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.app.greenchallenge">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GreenChallenge"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".desordenado.menumodule.news.NewsListActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".desordenado.treemodule.PaymentActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".desordenado.treemodule.TreeActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".desordenado.menumodule.commonchallenge.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.viewpagermodule.ViewPagerActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".desordenado.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".desordenado.menumodule.graph.GraphActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.menumodule.commonchallenge.CommonChallengeActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.menumodule.dailychallenge.DailyChallengeActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.menumodule.profile.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.menumodule.MenuActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.landingmodule.LandingActivity"
            android:exported="false" />
        <activity
            android:name=".desordenado.loginmodule.LoginActivity"
            android:exported="false" />
    </application>

</manifest>